#include<stdio.h>
#include<conio.h>
void main ()
{
int a[10],b[10],i,j,n,m;
clrscr();
printf("Enter the number of elements in domain but no more tha 10\n");
scanf("%d",&n);
printf("enter the elements now\n");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("Enter the number of elements in co domain but no more tha 10\n");
scanf("%d",&m);
printf("enter the elements now\n");
for(j=0;j<m;j++)
{
scanf("%d",&b[j]);
}
for(i=0;i<n;i++)
{
for (j=0;j<m;j++)
{
if(b[j]+a[i]==4)
printf("%d,%d\t",a[i],b[j]);
}
}
getch ();
}
